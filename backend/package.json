{
  "name": "backend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "rimraf tmp && npm run swagger && nodemon --inspect",
    "start:prod": "npm run swagger && npm run build && node build/app.js",
    "build": "rimraf build && tsc -b",
    "lint": "eslint .",
    "prettier-format": "prettier --config .prettierrc '*.ts' --write",
    "swagger:edit": "swagger project edit",
    "swagger": "js-yaml src/api/swagger/swagger.yaml > src/api/swagger/swagger.json",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "postbuild": "yarn run copy-files",
    "copy-files": "copyfiles -u 1 \"src/public/**/*\" build"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.0.20",
    "@ffprobe-installer/ffprobe": "^1.1.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "canvas": "^2.6.1",
    "capture-website": "^1.2.7",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "jsonwebtoken": "^8.5.1",
    "jszip": "^3.5.0",
    "md5": "^2.3.0",
    "mongodb": "^3.6.3",
    "monk": "^7.3.2",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "path": "^0.12.7",
    "socket.io": "^3.1.1",
    "supertest": "^6.1.1",
    "swagger-ui-express": "^4.1.5",
    "videoshow": "^0.1.12"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/express-fileupload": "^1.1.6",
    "@types/fluent-ffmpeg": "^2.1.16",
    "@types/jest": "^26.0.19",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/jszip": "^3.4.1",
    "@types/mongodb": "^3.6.3",
    "@types/node": "^14.14.12",
    "@types/node-fetch": "^2.5.8",
    "@types/socket.io": "^2.1.13",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "copyfiles": "^2.4.1",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "js-yaml": "^3.14.1",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "swagger": "^0.7.5",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2"
  }
}
